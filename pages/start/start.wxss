/**start.wxss**/
page,.container{
  background-color: #f0efeb;
  width: 100%;
  height: 100%;
  display: block;
  position: relative;
}
.bgImages{
  width: 100%;
  height: 70%;
  display: block;
}
.logo{
  width:100%;
  height:30%;
  color:#333;
  background-color:#ffffff;
  text-align:center;
  position: absolute;
  bottom: 0;
}
.title{
  font-size:40rpx;
  margin-top:55px;
  background-color:#b5272d;
  display:inline-block;
  color:#fff;
  padding:5px 8px;
  border-radius:3px;
}
.subtitle{
  font-size:28rpx;
  color:#666;
  margin-top: 15px;
}
.gohome{
  font-size:24rpx;
  background-color:rgba(0,0,0,.3);
  color:#fff;
  width:90px;
  text-align:center;
  float:right;
  position:absolute;
  right:10px;
  bottom:31%;
  border-radius:12px;
  padding:3px 0;
}